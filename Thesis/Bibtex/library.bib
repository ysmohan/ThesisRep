journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Aging,Aging: physiology,Animals,Brain Mapping,Ferrets,Neurons,Neurons: physiology,Orientation,Orientation: physiology,Photic Stimulation,Vision, Binocular,Visual Cortex,Visual Cortex: growth {\&} development,Visual Cortex: physiology},
month = {mar},
number = {5},
pages = {2609--14},
pmid = {9482934},
title = {{Overrepresentation of horizontal and vertical orientation preferences in developing ferret area 17.}},
