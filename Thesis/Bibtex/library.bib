month = {aug},
number = {4},
pages = {508--22},
pmid = {19709632},
publisher = {Elsevier Ltd},
title = {{Direct activation of sparse, distributed populations of cortical neurons by electrical microstimulation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2874753{\&}tool=pmcentrez{\&}rendertype=abstract},
