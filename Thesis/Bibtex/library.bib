journal = {The Journal of comparative neurology},
keywords = {Animals,Brain Mapping,Dominance, Cerebral,Dominance, Cerebral: physiology,Macaca fascicularis,Motion Perception,Motion Perception: physiology,Nerve Fibers, Myelinated,Nerve Fibers, Myelinated: physiology,Nerve Fibers, Myelinated: ultrastructure,Neurons,Neurons: physiology,Neurons: ultrastructure,Temporal Lobe,Temporal Lobe: anatomy {\&} histology,Temporal Lobe: physiology,Visual Cortex,Visual Cortex: anatomy {\&} histology,Visual Cortex: physiology,Visual Pathways,Visual Pathways: anatomy {\&} histology,Visual Pathways: physiology},
month = {jul},
number = {3},
pages = {293--326},
pmid = {7263951},
title = {{The middle temporal visual area in the macaque: myeloarchitecture, connections, functional properties and topographic organization.}},
