journal = {Journal of neurophysiology},
keywords = {Animals,Cerebral Cortex,Cerebral Cortex: cytology,Cerebral Cortex: physiology,Electric Stimulation,Humans,Magnetic Resonance Imaging,Microelectrodes,Neurons,Neurons: physiology},
