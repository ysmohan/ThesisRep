pages = {441--52},
pmid = {4024463},
title = {{Contour, color and shape analysis beyond the striate cortex}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/4024463 http://www.sciencedirect.com/science/article/pii/0042698985900690},
volume = {25},
