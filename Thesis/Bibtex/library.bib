journal = {Journal of neuroscience methods},
keywords = {Animals,Corpus Striatum,Corpus Striatum: cytology,Haplorhini,Immunohistochemistry,Immunohistochemistry: instrumentation,Mice,Microscopy, Fluorescence,Microscopy, Fluorescence: instrumentation,Optic Nerve,Optic Nerve: cytology,Photography},
month = {feb},
